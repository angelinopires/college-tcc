{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@admin": [
        "src/app/modules/admin"
      ],
      "@admin/*": ["src/app/modules/admin/*"],
      "@home": [
        "src/app/modules/home"
      ],
      "@home/*": [
        "src/app/modules/home/*"
      ],
      "@services": [
        "src/app/services"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@shared": [
        "src/app/shared"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@user": [
        "src/app/user"
      ],
      "@user/*": [
        "src/app/modules/user/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"]
  }
}
