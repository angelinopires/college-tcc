{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@home": [
        "src/app/modules/home"
      ],
      "@home/*": [
        "src/app/modules/home/*"
      ],
      "@materials": [
        "src/app/modules/materials"
      ],
      "@materials/*": [
        "src/app/modules/materials/*"
      ],
      "@orders": [
        "src/app/modules/orders"
      ],
      "@orders/*": [
        "src/app/modules/orders/*"
      ],
      "@prices": [
        "src/app/modules/prices"
      ],
      "@prices/*": [
        "src/app/modules/prices/*"
      ],
      "@providers": [
        "src/app/modules/providers"
      ],
      "@providers/*": [
        "src/app/modules/providers/*"
      ],
      "@requests": [
        "src/app/modules/requests"
      ],
      "@requests/*": [
        "src/app/modules/requests/*"
      ],
      "@services": [
        "src/app/services"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@shared": [
        "src/app/shared"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@users": [
        "src/app/modules/users"
      ],
      "@users/*": [
        "src/app/modules/users/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"]
  }
}
