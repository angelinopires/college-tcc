{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@authentication": [
        "src/app/modules/authentication"
      ],
      "@authentication/*": [
        "src/app/modules/authentication/*"
      ],
      "@enums": [
        "src/app/enums"
      ],
      "@enums/*": [
        "src/app/enums/*"
      ],
      "@home": [
        "src/app/modules/home"
      ],
      "@home/*": [
        "src/app/modules/home/*"
      ],
      "@materials": [
        "src/app/modules/materials"
      ],
      "@materials/*": [
        "src/app/modules/materials/*"
      ],
      "@material-ui": [
        "src/app/modules/material-ui"
      ],
      "@material-ui/*": [
        "src/app/modules/material-ui/*"
      ],
      "@mocks": [
        "src/app/mocks"
      ],
      "@mocks*": [
        "src/app/mocks/*"
      ],
      "@orders": [
        "src/app/modules/orders"
      ],
      "@orders/*": [
        "src/app/modules/orders/*"
      ],
      "@prices": [
        "src/app/modules/prices"
      ],
      "@prices/*": [
        "src/app/modules/prices/*"
      ],
      "@providers": [
        "src/app/modules/providers"
      ],
      "@providers/*": [
        "src/app/modules/providers/*"
      ],
      "@requests": [
        "src/app/modules/requests"
      ],
      "@requests/*": [
        "src/app/modules/requests/*"
      ],
      "@services": [
        "src/app/services"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@shared": [
        "src/app/shared"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@pipes": [
        "src/app/types"
      ],
      "@pipes/*": [
        "src/app/pipes/*"
      ],
      "@projectTypes": [
        "src/app/pipes"
      ],
      "@projectTypes/*": [
        "src/app/types/*"
      ],
      "@users": [
        "src/app/modules/users"
      ],
      "@users/*": [
        "src/app/modules/users/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
